<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
   	          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
 
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		
		<!-- ################ Login Private Area ################ -->
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/xhtml/login.xhtml</to-view-id>
		</navigation-case>
		
		<!-- ################ Site ################ -->
		<navigation-case>
			<from-outcome>dashBoard</from-outcome>
	   		<to-view-id>/xhtml/index.xhtml</to-view-id>
		</navigation-case>
		
		<!-- ################ UPLOAD ################ -->
		<navigation-case>
			<from-outcome>lapTrackUpload</from-outcome>
	   		<to-view-id>/xhtml/laptrack/upload.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>lapTrackList</from-outcome>
	   		<to-view-id>/xhtml/laptrack/listing.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>lapTrackConsult</from-outcome>
	   		<to-view-id>/xhtml/laptrack/consult.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
</faces-config>